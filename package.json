{
  "name": "next-model",
  "version": "0.6.1",
  "description": "Rails like models using TypeScript/ES6 and Decorators.",
  "author": "Tamino Martinius <dev@zaku.eu>",
  "main": "./dist/next_model.js",
  "module": "./dist/next_model.js",
  "jsnext:main": "./dist/next_model.js",
  "typings": "./dist/next_model.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamino-martinius/node-next-model.git"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "scripts": {
    "coverage": "rm -rf coverage* && jest --coverage --verbose && node remap_coverage.js && rm -rf coverage_jest_raw*",
    "test": "jest --verbose",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "@types/lodash": "^4.14.77",
    "@types/node": "^8.0.33",
    "@types/pluralize": "0.0.27",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "jest": "^21.2.1",
    "npm-watch": "^0.3.0",
    "remap-istanbul": "^0.9.5",
    "ts-jest": "^21.1.2",
    "typescript": "^2.5.3"
  },
  "peerDependencies": {
    "lodash": "^4.12.0",
    "pluralize": "^3.1.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coverageDirectory": "coverage_jest_raw"
  }
}
